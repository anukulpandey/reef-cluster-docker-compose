# SSH User for the container
SSH_USER=reefuser
SSH_PASS=reefpass
SSH_PORT=2222                   # Host port mapped to container SSH

# Container name (optional, useful to avoid conflicts)
CONTAINER_NAME=ubuntu-dev

# Node ports
P2P_PORT=30333                  # P2P port for this node
RPC_PORT=9944                   # RPC port for this node
SELF_NODE_P2P_PORT=30334        # Additional P2P port for bootstrap node
SELF_NODE_RPC_PORT=9945         # Additional RPC port for bootstrap node
